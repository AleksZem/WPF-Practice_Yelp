                        <Button x:Name="ShowsCitytatistics" Height="100" FontSize="40" Click="ShowLoggedInCityStatistics_Click">Show City Statistics</Button>
                    </StackPanel>
                </GroupBox>
                <GroupBox Header="User Information" Grid.Row="0" Grid.Column="4" Grid.ColumnSpan="4">
